<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".ui.employee.settings.SettingsEmployee"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">
<FrameLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
  >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"

        android:layout_gravity="center">
        <LinearLayout

            android:id="@+id/appBarLayout"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_35sdp"
            android:background="@color/blue"
            android:gravity="center"
            android:orientation="horizontal"
            android:padding="@dimen/_8sdp"
            android:paddingStart="@dimen/_16sdp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <TextView
                android:id="@+id/textView68"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:textSize="@dimen/_12ssp"
                android:text="اخرى"
                android:textColor="@color/White"
                android:textStyle="bold" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/white"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="@dimen/_300sdp"
                android:layout_height="@dimen/_35sdp"
                android:gravity="center"
                android:elevation="@dimen/_1sdp"
                android:layout_gravity="center|top"
                android:background="@drawable/white_bac"
                android:layout_marginTop="@dimen/_10sdp"
                android:layout_margin="5dp"
                android:orientation="horizontal">



                <TextView
                    android:id="@+id/textView64"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_10sdp"
                    android:textColor="@color/primary_dark"
                    android:layout_weight="1"
                    android:text="الملف الشخصي " />
                <ImageView
                    android:id="@+id/imageView22"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="5dp"
                    android:layout_marginStart="@dimen/_20sdp"
                    android:src="@drawable/user_button" />

            </LinearLayout>
            <LinearLayout
                android:id="@+id/addDonation"
                android:layout_width="@dimen/_300sdp"
                android:layout_height="@dimen/_35sdp"
                android:layout_margin="5dp"
                android:gravity="center"
                android:layout_gravity="center|top"
                android:elevation="@dimen/_1sdp"
                android:background="@drawable/white_bac"
                android:layout_marginTop="@dimen/_5sdp"
                android:orientation="horizontal">



                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_10sdp"
                    android:textColor="@color/primary_dark"
                    android:layout_weight="1"
                    android:text="اضافة تبرع" />
                <ImageView
                    android:layout_width="@dimen/_15sdp"
                    android:layout_margin="5dp"
                    android:layout_height="@dimen/_15sdp"
                    android:layout_marginStart="@dimen/_20sdp"
                    android:src="@drawable/ic_icon_map_location_arrow" />

            </LinearLayout>
            <LinearLayout
                android:layout_width="@dimen/_300sdp"
                android:layout_height="@dimen/_35sdp"
                android:gravity="center"
                android:layout_margin="5dp"
                android:layout_gravity="center|top"
                android:background="@drawable/white_bac"
                android:elevation="@dimen/_1sdp"
                android:layout_marginTop="@dimen/_5sdp"
                android:orientation="horizontal">


                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_10sdp"
                    android:textColor="@color/primary_dark"
                    android:layout_weight="1"
                    android:text="الاشعارات" />
                <ImageView
                    android:layout_margin="5dp"
                    android:layout_width="@dimen/_15sdp"
                    android:layout_height="@dimen/_15sdp"
                    android:layout_marginStart="@dimen/_20sdp"
                    android:src="@drawable/noti_pressed_2" />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/appliedform"
                android:layout_width="@dimen/_300sdp"
                android:layout_height="@dimen/_35sdp"
                android:gravity="center"
                android:layout_margin="5dp"
                android:layout_gravity="center|top"
                android:background="@drawable/white_bac"
                android:layout_marginTop="@dimen/_5sdp"
                android:elevation="@dimen/_1sdp"

                android:orientation="horizontal">



                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_10sdp"
                    android:textColor="@color/primary_dark"
                    android:layout_weight="1"
                    android:text="طلب معونة" />
                <ImageView
                    android:layout_margin="5dp"
                    android:layout_width="@dimen/_15sdp"
                    android:layout_height="@dimen/_15sdp"
                    android:layout_marginStart="@dimen/_20sdp"
                    android:src="@drawable/ic_ques" />

            </LinearLayout>
            <LinearLayout
                android:layout_margin="5dp"
                android:layout_width="@dimen/_300sdp"
                android:layout_height="@dimen/_35sdp"
                android:gravity="center"
                android:layout_gravity="center|top"
                android:background="@drawable/white_bac"
                android:layout_marginTop="@dimen/_5sdp"
                android:elevation="@dimen/_1sdp"

                android:orientation="horizontal">



                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_10sdp"
                    android:textColor="@color/primary_dark"
                    android:layout_weight="1"
                    android:text="تواصل معنا" />
                <ImageView
                    android:layout_margin="5dp"
                    android:layout_width="@dimen/_15sdp"
                    android:layout_height="@dimen/_15sdp"
                    android:layout_marginStart="@dimen/_20sdp"
                    android:src="@drawable/ic_phone_call" />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/signOutBtn"
                android:layout_width="@dimen/_300sdp"
                android:layout_height="@dimen/_35sdp"
                android:gravity="center"

                android:layout_gravity="center|top"
                android:elevation="@dimen/_1sdp"
                android:background="@drawable/white_bac"
                android:layout_marginTop="@dimen/_25sdp"
                android:layout_margin="5dp"
                android:orientation="horizontal">


                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_10sdp"
                    android:textColor="@color/primary_dark"
                    android:layout_weight="1"
                    android:text="تسجيل الخروج" />
                <ImageView
                    android:layout_margin="5dp"
                    android:layout_width="@dimen/_15sdp"
                    android:layout_height="@dimen/_15sdp"
                    android:layout_marginStart="@dimen/_20sdp"
                    android:src="@drawable/ic_sign_out" />

            </LinearLayout>


        </LinearLayout>

        <LinearLayout
            android:id="@+id/acceptapplied"
            android:layout_width="@dimen/_300sdp"
            android:layout_height="@dimen/_35sdp"
            android:gravity="center"

            android:layout_gravity="center|top"
            android:elevation="@dimen/_1sdp"
            android:background="@drawable/white_bac"
            android:layout_marginTop="@dimen/_25sdp"
            android:layout_margin="5dp"
            android:orientation="horizontal">


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_10sdp"
                android:textColor="@color/primary_dark"
                android:layout_weight="1"
                android:text="واجهة مسؤؤل التنمية" />

            <ImageView
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_margin="5dp"
                android:layout_marginStart="@dimen/_20sdp"
                android:src="@drawable/ic_baseline_rule_folder_24" />

        </LinearLayout>
        <LinearLayout
            android:id="@+id/associationsbtn"
            android:layout_width="@dimen/_300sdp"
            android:layout_height="@dimen/_35sdp"
            android:gravity="center"

            android:layout_gravity="center|top"
            android:elevation="@dimen/_1sdp"
            android:background="@drawable/white_bac"
            android:layout_marginTop="@dimen/_25sdp"
            android:layout_margin="5dp"
            android:orientation="horizontal">


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_10sdp"
                android:textColor="@color/primary_dark"
                android:layout_weight="1"
                android:text="واجهة مسؤؤل الجمعية" />

            <ImageView
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_margin="5dp"
                android:layout_marginStart="@dimen/_20sdp"
                android:src="@drawable/ic_baseline_rule_folder_24" />

        </LinearLayout>

    </LinearLayout>



</FrameLayout>
    </ScrollView>