<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/carViewApplicants"
    android:layout_width="match_parent"
    android:layout_height="@dimen/_115sdp"
    android:layout_marginTop="@dimen/_5sdp"
    android:orientation="vertical">


    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginStart="@dimen/_5sdp"
        android:layout_marginEnd="@dimen/_5sdp"
        android:layout_marginTop="@dimen/_3sdp"
        android:layout_marginBottom="@dimen/_3sdp">

        <de.hdodenhof.circleimageview.CircleImageView
            android:id="@+id/EmployeePhoto"
            android:layout_width="@dimen/_40sdp"
            android:layout_height="@dimen/_40sdp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="8dp"
            android:src="@drawable/defult_user_photo"
            app:civ_border_color="@color/primary_dark"
            app:civ_border_width="0.5dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints" />

        <TextView
            android:id="@+id/EmployeeName"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:text="Employee Name"
            android:textColor="@color/primary_dark"
            android:textSize="@dimen/_13ssp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/EmployeePhoto"
            app:layout_constraintStart_toEndOf="@+id/EmployeePhoto"
            app:layout_constraintTop_toTopOf="@+id/EmployeePhoto"
            app:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/ExperianceApplicants"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Experiance"
            android:textColor="@color/blue"
            app:layout_constraintEnd_toEndOf="@+id/EmployeeName"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/EmployeeName"
            app:layout_constraintTop_toBottomOf="@+id/EmployeeName" />

        <ImageView
            android:id="@+id/imageView19"
            android:layout_width="@dimen/_13sdp"
            android:layout_height="@dimen/_13sdp"
            android:layout_marginTop="8dp"
            app:layout_constraintEnd_toEndOf="@+id/EmployeePhoto"
            app:layout_constraintStart_toStartOf="@+id/EmployeePhoto"
            app:layout_constraintTop_toBottomOf="@+id/EmployeePhoto"
            app:srcCompat="@drawable/new_time"
            tools:ignore="VectorDrawableCompat" />

        <TextView
            android:id="@+id/workTime"
            android:layout_width="@dimen/_210sdp"
            android:layout_height="@dimen/_15sdp"
            android:layout_marginStart="8dp"
            android:textAlignment="viewStart"
            android:text="Full time, Part time"
            android:textSize="@dimen/_9ssp"
            app:layout_constraintBottom_toBottomOf="@+id/imageView19"
            app:layout_constraintStart_toEndOf="@+id/imageView19"
            app:layout_constraintTop_toTopOf="@+id/imageView19" />

        <ImageView
            android:id="@+id/imageView18"
            android:layout_width="@dimen/_13sdp"
            android:layout_height="@dimen/_13sdp"
            android:layout_marginTop="8dp"
            app:layout_constraintEnd_toEndOf="@+id/imageView19"
            app:layout_constraintStart_toStartOf="@+id/imageView19"
            app:layout_constraintTop_toBottomOf="@+id/imageView19"
            app:srcCompat="@drawable/ic_icon_map_location_arrow"
            tools:ignore="VectorDrawableCompat" />

        <TextView
            android:id="@+id/location"
            android:layout_width="@dimen/_210sdp"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:text="Dubai"
            android:textAlignment="viewStart"
            android:textSize="@dimen/_9ssp"
            app:layout_constraintBottom_toBottomOf="@+id/imageView18"
            app:layout_constraintStart_toEndOf="@+id/imageView18"
            app:layout_constraintTop_toTopOf="@+id/imageView18" />

        <ImageView
            android:id="@+id/imageView15"
            android:layout_width="@dimen/_13sdp"
            android:layout_height="@dimen/_13sdp"
            android:layout_marginTop="8dp"
            app:layout_constraintEnd_toEndOf="@+id/imageView18"
            app:layout_constraintStart_toStartOf="@+id/imageView18"
            app:layout_constraintTop_toBottomOf="@+id/imageView18"
            app:srcCompat="@drawable/ic_calendar_new" />

        <TextView
            android:id="@+id/day"
            android:layout_width="@dimen/_210sdp"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:text="yesterday"
            android:textAlignment="viewStart"
            android:textSize="@dimen/_9ssp"
            app:layout_constraintBottom_toBottomOf="@+id/imageView15"
            app:layout_constraintStart_toEndOf="@+id/imageView15"
            app:layout_constraintTop_toTopOf="@+id/imageView15" />


        <ImageView
            android:id="@+id/settings"
            android:layout_width="@dimen/_10sdp"
            android:layout_height="@dimen/_15sdp"
            android:layout_marginEnd="16dp"
            app:layout_constraintBottom_toBottomOf="@+id/EmployeePhoto"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/EmployeePhoto"
            app:layout_constraintVertical_bias="0.06999999"
            app:srcCompat="@drawable/points" />

        <TextView
            android:id="@+id/textView61"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="32dp"
            tools:text="TextView"
            android:textColor="@color/md_red_800"
            app:layout_constraintBottom_toBottomOf="@+id/EmployeeName"
            app:layout_constraintStart_toEndOf="@+id/EmployeeName"
            app:layout_constraintTop_toTopOf="@+id/EmployeeName" />
    </androidx.constraintlayout.widget.ConstraintLayout>



</androidx.cardview.widget.CardView>